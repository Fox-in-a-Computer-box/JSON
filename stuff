import urllib.request, urllib.parse, urllib.error
import json

urlinp = input("Enter a URL:")

urlopn = urllib.request.urlopen(urlinp)

urlread = urlopn.read()

datajson = json.loads(urlread)

#print(datajson)

#print(datajson['comments'])

for item in datajson['comments']:
    print(item['count'])
